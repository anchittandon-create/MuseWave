#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Anchit.Tandon/Desktop/AI HUSTLE - APPS/MuseWave3/node_modules/.pnpm/vite@6.4.1_@types+node@22.18.11_jiti@1.21.7_tsx@4.20.6/node_modules/vite/bin/node_modules:/Users/Anchit.Tandon/Desktop/AI HUSTLE - APPS/MuseWave3/node_modules/.pnpm/vite@6.4.1_@types+node@22.18.11_jiti@1.21.7_tsx@4.20.6/node_modules/vite/node_modules:/Users/Anchit.Tandon/Desktop/AI HUSTLE - APPS/MuseWave3/node_modules/.pnpm/vite@6.4.1_@types+node@22.18.11_jiti@1.21.7_tsx@4.20.6/node_modules:/Users/Anchit.Tandon/Desktop/AI HUSTLE - APPS/MuseWave3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Anchit.Tandon/Desktop/AI HUSTLE - APPS/MuseWave3/node_modules/.pnpm/vite@6.4.1_@types+node@22.18.11_jiti@1.21.7_tsx@4.20.6/node_modules/vite/bin/node_modules:/Users/Anchit.Tandon/Desktop/AI HUSTLE - APPS/MuseWave3/node_modules/.pnpm/vite@6.4.1_@types+node@22.18.11_jiti@1.21.7_tsx@4.20.6/node_modules/vite/node_modules:/Users/Anchit.Tandon/Desktop/AI HUSTLE - APPS/MuseWave3/node_modules/.pnpm/vite@6.4.1_@types+node@22.18.11_jiti@1.21.7_tsx@4.20.6/node_modules:/Users/Anchit.Tandon/Desktop/AI HUSTLE - APPS/MuseWave3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@6.4.1_@types+node@22.18.11_jiti@1.21.7_tsx@4.20.6/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@6.4.1_@types+node@22.18.11_jiti@1.21.7_tsx@4.20.6/node_modules/vite/bin/vite.js" "$@"
fi
